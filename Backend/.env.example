# Google Gemini API Configuration
GOOGLE_API_KEY=your-google-api-key-here
GEMINI_MODEL=gemini-1.5-flash

# Ollama Configuration (Local LLM)
OLLAMA_MODEL=functiongemma
OLLAMA_BASE_URL=http://localhost:11434/v1

# Model Parameters
TEMPERATURE=0.7
TIMEOUT=300
API_HOST=0.0.0.0
API_PORT=8000

# ============================================================================
# Node.js Backend Configuration (app.js)
# ============================================================================

# Server Port
PORT=5000
NODE_ENV=development

# Local Supabase Configuration
# Get these values by running: npx supabase start
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_ANON_KEY=your-local-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-local-supabase-service-role-key

# Database Direct Connection (if needed)
DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=7d
